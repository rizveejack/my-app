{
  "kind": "collectionType",
  "collectionName": "shops",
  "info": {
    "singularName": "shop",
    "pluralName": "shops",
    "displayName": "Shop",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Location": {
      "type": "string",
      "required": true
    },
    "Verified": {
      "type": "boolean",
      "default": false
    },
    "Logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "shop"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "shop"
    }
  }
}
